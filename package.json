{
  "name": "visula",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.4.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint ./src",
    "ts-lint": "tsc --noEmit --incremental --watch",
    "generate": "antlr4 -Dlanguage=JavaScript -message-format vs2005 ./src/grammar/VisulaParser.g4 -o src/lib/",
    "db:migrate": "pnpx prisma migrate dev --create-only --name",
    "db:generate": "pnpx prisma generate",
    "db:studio": "pnpx prisma studio",
    "db:push": "pnpx prisma db push",
    "postinstall": "prisma generate"
  },
